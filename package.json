    "i18next": "^22.0.0",
    "rc-slider": "^10.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.38.0",
    "react-image-gallery": "^1.2.11",
    "react-responsive": "^9.0.0",
    "slick-carousel": "^1.8.1",
    "zod": "^3.19.1",
    "zod-i18n-map": "^1.3.0"
