  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=16.14.0"
  },
  "scripts": {
    "postinstall": "patch-package",
    "generate": "graphql-codegen --config codegen.yml",
    "dev:graphql": "graphql-codegen --config codegen.yml --watch",
    "dev:hydrogen": "shopify hydrogen dev",
    "dev": "npm-run-all -p dev:*",
    "build": "yarn generate && shopify hydrogen build",
    "preview": "shopify hydrogen preview",
    "test": "playwright test",
    "test:update": "playwright test --update-snapshots"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.13.7",
    "@graphql-codegen/gql-tag-operations-preset": "^1.6.0",
    "@graphql-codegen/schema-ast": "^2.5.1",
    "@headlessui/react": "^1.7.3",
    "@playwright/test": "^1.27.1",
    "@shopify/cli": "3.18.0",
    "@shopify/cli-hydrogen": "3.18.0",
    "@tailwindcss/typography": "^0.5.7",
    "@types/react": "^18.0.21",
    "@types/react-image-gallery": "^1.0.5",
    "@types/react-slick": "^0.23.10",
    "autoprefixer": "^10.4.12",
    "npm-run-all": "^4.1.5",
    "patch-package": "^6.4.7",
    "playwright": "^1.27.1",
    "postcss": "^8.4.18",
    "postinstall-postinstall": "^2.1.0",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.4",
    "vite": "^2.9.0",
    "vite-plugin-svgr": "^2.2.2"
